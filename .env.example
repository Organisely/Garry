# Garry Bot Environment Variables
# Copy this file to .env and fill in your values

# VCS Configuration
GARRY_VCS_TOKEN=your-github-token-here
GARRY_VCS_PLATFORM=github
GARRY_VCS_HOST=github.com
GARRY_VCS_REPOSITORY=organisely/your-repo

# Logging
RUST_LOG=info

# Bot Configuration (optional, defaults in config.toml)
# GARRY_BOT_WEBHOOK_PORT=8080
# GARRY_BOT_QUEUE_CHECK_INTERVAL=30
# GARRY_BOT_CI_TIMEOUT=3600
# GARRY_BOT_MAIN_BRANCH=main
